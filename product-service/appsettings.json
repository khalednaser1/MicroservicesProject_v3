{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Metrics": {
    "Enabled": true,
    "Port": 6001
  },
  "HealthChecks": {
    "Endpoint": "/health",
    "Enabled": true
  }
}
